public class Hello
{
    private String Message = "";
    public Hello()
    {
        Message = "Hello";
    }
    
    public void changeMessage(String str)
    {
        Message = str;
    }
    
    public void origMes()
    {
        Message = "Heeeeeeeeelllllllooooooo";
    }
    
}
